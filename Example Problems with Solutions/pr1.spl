-- Defines the columns from the problem file
col1 = problem !! 1;
col2 = problem !! 2;
x=1;
y=1;
-- Loops through the length of the list, outputting col1 and 2 on even numbers, and just 1 on odd
for x to (length col1) {
    if (x % 2) == 0 {
        print (col1 !! x);
        print (col2 !! y);
        y=y+1;
    }else {
        print (col1 !! x);
    };
    x=x+1;
};
